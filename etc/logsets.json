[
  {
    "name": "hagfish_logs",
    "search_dirs": [ "/var/log/usage" ],
    "regex": "^/var/log/usage/([0-9]+)/([0-9]+)/([0-9]+)/([a-f0-9-]+)\\.([0-9T]+)\\.log\\.gz$",
    "manta_path": "/%u/stor/sdc/usage/%d/$4/$1/$2/$3/$5.log.gz",
    "debounce_time": 600,
    "zones": [
      "global"
    ]
  },
  {
    "name": "sdc_zone_logs",
    "search_dirs": [ "/var/log/sdc/upload" ],
    "regex": "^/var/log/sdc/upload/([a-zA-Z0-9-]+)_([0-9a-f-]+)_([0-9]+)-([0-9]+)-([0-9]+)T([0-9]+):00:00\\.log$",
    "date_string": {
      "y": "$3", "m": "$4", "d": "$5",
      "H": "$6"
    },
    "date_adjustment": "-1H",
    "manta_path": "/%u/stor/logs/%d/$1/#y/#m/#d/#H/$2.log",
    "debounce_time": 600,
    "zones": [
      "adminui",
      "amon",
      "amonredis",
      "binder",
      "ca",
      "cloudapi",
      "cnapi",
      "dapi",
      "dhcpd",
      "fwapi",
      "imgapi",
      "keyapi",
      "manatee",
      "manta",
      "moray",
      "napi",
      "papi",
      "rabbitmq",
      "redis",
      "sapi",
      "sdc",
      "sdcsso",
      "ufds",
      "usageapi",
      "vmapi",
      "workflow"
    ]
  }
]
